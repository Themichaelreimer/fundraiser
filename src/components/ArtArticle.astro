---
import ArtistBio from "./ArtistBio.astro";
import ArtItem from "./ArtItem.astro";


    const {name, description, imagePath, artContentType, colspan, link} = Astro.props;

---
{ artContentType=="biography" && <ArtistBio name={name} description={description} imagePath={imagePath} link={link}></ArtistBio> }
{ artContentType=="item" && <ArtItem name={name} description={description} imagePath={imagePath} colspan={colspan} link={link}></ArtItem>}
