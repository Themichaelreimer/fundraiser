---
import ArtistBio from "./ArtistBio.astro";
import ArtItem from "./ArtItem.astro";


    const {name, description, imagePath, artContentType, colspan} = Astro.props;

---
{ artContentType=="biography" && <ArtistBio name={name} description={description} imagePath={imagePath}></ArtistBio> }
{ artContentType=="item" && <ArtItem name={name} description={description} imagePath={imagePath} colspan={colspan}></ArtItem>}
